<%- include('../../partials/navbar.ejs') %>
<div class="container">
    <hr>
    <h2>Usuarios</h2>
    <a class="btn btn-primary" href="/admin/users/create">Criar novo usuario</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% user.forEach(user =>{ %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                </th>
            <% })%> 
        </tbody>
    </table>
</div>
